{
  "name": "axios-enhance-adapter",
  "description": "An axios adapter to avoid repeat request at same time and support erry retry",
  "version": "0.0.12",
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs",
    "module": "./dist/index.esm.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suhaotian/axios-enhance-adapter.git"
  },
  "author": {
    "name": "@suhaotian"
  },
  "bugs": {
    "url": "https://github.com/suhaotian/axios-enhance-adapter/issues"
  },
  "keywords": [
    "axios",
    "adapter",
    "request"
  ],
  "scripts": {
    "build": "bunchee ./src/index.ts",
    "dev": "ts-node ./src/main.ts",
    "test": "cross-env NODE_ENV=test vitest run",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "start-publish": "cp ../../README.md ./ && node scripts/remove-local-deps.mjs && npm run build && npm publish --registry=https://registry.npmjs.org && node scripts/restore-local-deps.mjs"
  },
  "dependencies": {},
  "devDependencies": {
    "bunchee": "^3.1.1",
    "axios": "^0.27.2",
    "express": "^4.18.2",
    "puppeteer": "^13.6.0"
  },
  "peerDependencies": {
    "axios": "0.27.2"
  },
  "eslintConfig": {
    "extends": [
      "@configs/eslint-config/node"
    ],
    "ignorePatterns": [
      "node_modules"
    ]
  },
  "license": "MIT"
}
